<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.litfinder.view.main.BerandaFragment"
        android:label="home"
        tools:layout="@layout/fragment_beranda" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.litfinder.view.main.DiscoverFragment"
        android:label="setting"
        tools:layout="@layout/fragment_discover" />

    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.example.litfinder.view.main.BookshelfFragment"
        android:label="notification"
        tools:layout="@layout/fragment_bookshelf" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.example.litfinder.view.main.ProfileFragment"
        android:label="history"
        tools:layout="@layout/fragment_profile" />

</navigation>